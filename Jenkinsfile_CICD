pipeline {
    agent any
    stages {
        stage('Git checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/rohitG7496/K8S-3-Tier_DevSecOps_Project.git'
            }
        } // close Git checkout
        stage('sonarqube code analysis') {
            steps {
                withSonarQubeEnv('sonarqube') {
                    sonarqubeScanner 'sonarqube'
                    
                }
            }
        }
    } //close stages
} //close pipeline
